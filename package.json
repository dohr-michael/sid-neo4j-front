    "babel-plugin-rewire": "^0.1.22",
    "babel-runtime": "^5.8.25",
    "css-loader": "^0.19.0",
    "eslint": "^1.7.1",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.2",
    "json-loader": "^0.5.3",
    "node-sass": "^3.3.3",
    "opn": "^3.0.2",
    "postcss-loader": "^0.6.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "template-html-loader": "0.0.3",
